      <facet id="facet1" version="2.0"/>
      <facet id="facet2" version="3.5#a"/>
      <facet id="facet2ext" version="1.0"/>
    </preset>

    <preset id="preset2">
      <facet id="facet3a" version="1.0"/>
      <facet id="facet3b" version="1.0"/>
      <facet id="facet3c" version="1.0"/>
    </preset>
    
